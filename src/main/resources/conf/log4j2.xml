<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" packages="hr.smilebacksmile.logging.frameworks.log4j.plugins">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <!--<PatternLayout pattern="%duration %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />-->
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="TRACE">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>